#include <iostream>
using namespace std;
int main() {
	string sinus[5][60];
	int i=0, j=0;

	for (i = 1; i < 4; i++)
	{
		for (j = 0; j < 51; j++)
		{
			sinus[i][j] = " ";
		}
	}

	for (j = 0; j < 51; j++)
	{
		if (j % 4 == 0 || j % 4 == 2)
		{
			sinus[2][j] = "*";
		}
		if (j % 4 == 1)
		{
			sinus[1][j] = "*";
		}
		if (j % 4 == 3)
		{
			sinus[3][j] = "*";
		}
	}

	for (i = 0; i < 4; i++)
	{
		for (j = 4; j < 51; j++)
		{
			cout << sinus[i][j];
		}
		cout << endl;
	}


	return 0;
}
